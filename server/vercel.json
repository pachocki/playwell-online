{
  "rewrites": [
    {
      "sources": "/api/(.*)",
      "destination": "/api/"

    }
  ],
  
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ]
}