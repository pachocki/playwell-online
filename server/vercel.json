{
  
  "version": 2,
  "rewrites":[
    {
      "source:":"/api/(.*)",
      "destination": "/api"
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    }
  ]
}